// To print data in the Array,for each,For loop

public class ArrayEx1 {
	public static void main(String[] args) {
		int a[] = new int[5];
		int b[] = new int[] { 2, 3, 4, 5, 6 };

		for (int x : b) {
			System.out.println(x);
		}

		for (int i=0; i<b.length;i++) {
			
		System.out.println(b[i]);
	}

	}}

